{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes":[
        "error","single"  //Dùng dấu nháy đơn hay nháy kép cho string
      ],
      "linebreak-style":"off", //Chỉ định dấu xuống dòng kiểu Windows CRLF hay Unix LF
      "camelcase":"off",
      "no-unused-vars": "off"
    }
}
